{
  "name": "generate-target",
  "description": "Plugin for automatically creating tasks from declarative `target` configurations. Works with generate, assemble, verb, or any other base application with plugin support.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/generate-target",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/generate-target",
  "bugs": {
    "url": "https://github.com/jonschlinkert/generate-target/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "generate",
    "target"
  ],
  "dependencies": {
    "base-files-each": "^0.1.5",
    "base-target": "^0.1.0",
    "is-valid-app": "^0.2.0",
    "mixin-deep": "^1.1.3"
  },
  "devDependencies": {
    "async-each-series": "^1.1.0",
    "base": "^0.11.1",
    "base-app": "^0.2.6",
    "expand-target": "^0.6.4",
    "fs-exists-sync": "^0.1.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3",
    "through2": "^2.0.1"
  },
  "verb": {
    "reflinks": [
      "assemble",
      "base",
      "expand-target",
      "generate",
      "generate-dest",
      "generate-install",
      "gulp"
    ],
    "toc": false,
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}